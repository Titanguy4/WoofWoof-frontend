{
  "name": "woofwoof",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "detox:build:android": "detox build --configuration android.emu.debug",
    "detox:test:android": "detox test --configuration android.emu.debug",
    "detox:build:ios": "detox build --configuration ios.sim.debug",
    "detox:test:ios": "detox test --configuration ios.sim.debug --testMatch '**/e2e/**/*.e2e.js'"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs}": [
      "npx expo lint",
      "npx prettier --write --loglevel=warn"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/manrope": "^0.4.2",
    "@expo/vector-icons": "^15.0.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "dayjs": "^1.11.19",
    "expo": "~54.0.12",
    "expo-auth-session": "~7.0.8",
    "expo-blur": "^15.0.7",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-router": "~6.0.10",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "i18next": "^25.6.2",
    "jwt-decode": "^4.0.0",
    "lucide-react-native": "^0.553.0",
    "nativewind": "^4.1.21",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.3.3",
    "react-native": "0.81.4",
    "react-native-calendars": "^1.1313.0",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-css": "^3.0.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-segmented-control-tab": "^4.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "detox": "^20.46.0",
    "detox-expo-helpers": "^0.6.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "expo-detox-hook": "^1.0.10",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.13",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.2"
  },
  "private": true,
  "detox": {
    "apps": {
      "ios.sim.debug": {
        "type": "ios.app",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/WoofWoof.app",
        "build": "xcodebuild -workspace ios/WoofWoof.xcworkspace -scheme WoofWoof -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build"
      }
    },
    "devices": {
      "simulator": {
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 14"
        }
      }
    },
    "configurations": {
      "ios.sim.debug": {
        "device": "simulator",
        "app": "ios.sim.debug"
      }
    }
  }
}
